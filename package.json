{
  "name": "astro-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .ts,.js",
    "build": "tsc && ts-node copy-assets.ts",
    "start": "cross-env NODE_ENV=production node dist/src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec 'ts-node' src/index.ts",
    "dev:3000": "cross-env NODE_ENV=development PORT=3000 nodemon --exec ts-node src/index.ts",
    "dev:5000": "cross-env NODE_ENV=development PORT=5000 ts-node src/index.ts",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles",
    "test:watch": "cross-env NODE_ENV=test nodemon --exec 'npm test' --ext ts,js,json"
  },
  "keywords": [],
  "author": "Ydv Sooraj",
  "license": "ISC",
  "description": "Subscription gateway for Astro backend",
